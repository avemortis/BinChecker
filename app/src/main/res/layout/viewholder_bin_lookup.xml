<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp"
    android:paddingTop="8dp">


    <TextView
        android:id="@+id/viewholderBinLookupSheme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/viewHolderBinLookupType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewholderBinLookupSheme" />

    <TextView
        android:id="@+id/viewholderBinLookupBrand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewHolderBinLookupType" />

    <TextView
        android:id="@+id/viewholderBinLookupPrepaid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewholderBinLookupBrand" />

    <TextView
        android:id="@+id/viewholderBinLookupCountry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewholderBinLookupPrepaid" />

    <TextView
        android:id="@+id/viewholderBinLookupBank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewholderBinLookupCountry" />

    <TextView
        android:id="@+id/viewholderBinLookupNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewholderBinLookupBank" />

    <Button
        android:id="@+id/viewholderBinLookupNumberButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewholderBinLookupNumber" />

    <Button
        android:id="@+id/viewholderBinLookupUrlButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Site"
        app:layout_constraintEnd_toStartOf="@+id/viewholderBinLookupNumberButton"
        app:layout_constraintStart_toEndOf="@+id/viewholderBinLookupGpsButton"
        app:layout_constraintTop_toTopOf="@+id/viewholderBinLookupNumberButton" />

    <Button
        android:id="@+id/viewholderBinLookupGpsButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/viewholderBinLookupUrlButton" />

</androidx.constraintlayout.widget.ConstraintLayout>